{
	"rewrites": [
		{
			"source": "/images/:file",
			"destination": "/images/normal/:file"
		}
	],
	"functions": {
		"api/pokemon/*.ts": {
			"runtime": "vercel-deno@0.7.5",
      "includeFiles": "api/pokemon/_data/**"
		},
		"api/types/*.ts": {
			"runtime": "vercel-deno@0.7.5",
      "includeFiles": "api/types/_data/**"
		}
	}
}
