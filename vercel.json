{
	"rewrites": [
		{
			"source": "/api/pokemon/:id.png",
			"destination": "/api/pokemon/img/:id"
		}
	],
	"functions": {
		"api/pokemon/img/*.ts": {
			"runtime": "vercel-deno@0.7.5",
      "includeFiles": "api/pokemon/img/_data/**"
		},
		"api/**/*.ts": {
			"runtime": "vercel-deno@0.7.5",
      "includeFiles": "api/pokemon/_data/**"
		}
	}
}
